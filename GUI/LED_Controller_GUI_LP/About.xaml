<Window x:Class="TPS9266xEvaluationModule.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPS9266xEvaluationModule"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="About" Height="250.002" Width="605.922" ResizeMode="NoResize">
    <Border BorderBrush="Black" BorderThickness="2">
        <Grid x:Name="gridAboutBox" Margin="0">
            <Grid.Background>
                <ImageBrush ImageSource="SplashScreen682.png" Stretch="Uniform" AlignmentX="Left">
                    <ImageBrush.RelativeTransform>
                        <TransformGroup>
                            <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                            <SkewTransform CenterY="0.5" CenterX="0.5"/>
                            <RotateTransform CenterY="0.5" CenterX="0.5"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </ImageBrush.RelativeTransform>
                </ImageBrush>
            </Grid.Background>
            <StackPanel x:Name="atackPanelAboutInfo" Margin="287,2,0,47" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Width="294">
                <StackPanel.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="0.257"/>
                        <RotateTransform/>
                        <TranslateTransform X="-0.334"/>
                    </TransformGroup>
                </StackPanel.RenderTransform>
                <Label x:Name="labelProductName" Content="Label"/>
                <Label x:Name="labelRevs" Content="Label"/>
                <Label x:Name="labelCopyright" Content="Label"/>
                <Label x:Name="labelCompanyName" Content="Label"/>
                <TextBox x:Name="textBoxDescription" Height="49" TextWrapping="Wrap" Text="TextBox" Margin="3,3,10,3"/>
            </StackPanel>
            <Button x:Name="buttonOK" Content="_OK" HorizontalAlignment="Left" Margin="508,181,0,0" VerticalAlignment="Top" Width="54" Click="buttonOK_Click" RenderTransformOrigin="2.954,0.511"/>
        </Grid>
    </Border>
</Window>
